{
  "name": "credit-appl",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "drcp": "drcp",
    "clean:dist": "drcp clean",
    "start:dev-proxy": "node app -c dist/config.local.yaml dist/dev-proxy.yaml",
    "e2e:dry": "npm run protractor",
    "e2e:debug": "node --inspect-brk ./node_modules/.bin/protractor ./e2e/protractor.conf.js",
    "protractor": "ng e2e -c dry",
    "e2e:byj": "ng e2e -c byj",
    "ba": "webpack-bundle-analyzer dist/static/credit-appl/stats.json",
    "build": "drcp init && node node_modules/@wfh/assets-processer/dist/unzip.js",
    "stop": "pm2 kill",
    "init": "npm i -g yarn && npm i -g npm@6.11.3",
    "e2e": "ng e2e -c test",
    "start:dell": "pm2 update && pm2 start ecosystem.config.js --only log4js-intercom && pm2 start ecosystem.config.js --only credit-appl-local",
    "stop:dell": "pm2 delete credit-appl-local && pm2 delete log4js-intercom",
    "start:stark": "bash scripts/start-pm2-stark.sh",
    "start:prod": "pm2 start ecosystem.config.js --only log4js-intercom && pm2 start ecosystem.config.js --only credit-appl-prod --no-daemon",
    "start:dev": "pm2 start ecosystem.config.js --only log4js-intercom && pm2 start ecosystem.config.js --only credit-appl-dev --no-daemon",
    "start:test": "pm2 start ecosystem.config.js --only log4js-intercom && pm2 start ecosystem.config.js --only credit-appl-test --no-daemon",
    "start:stage": "pm2 start ecosystem.config.js --only log4js-intercom && pm2 start ecosystem.config.js --only credit-appl-dev --no-daemon",
    "start:local": "pm2 start ecosystem.config.js --only log4js-intercom && pm2 start ecosystem.config.js --only credit-appl-local --no-daemon",
    "build:stark": "bash ./scripts/devops-build.sh",
    "prettier": "npx prettier --config-precedence prefer-file --config ./prettier.config.js --ignore-path ./prettierignore  \"./**/*.{ts,json,less,scss}\" --write"
  },
  "private": true,
  "packages": [
    "admin/src",
    "projects",
    "docs"
  ],
  "dependencies": {
    "@angular/animations": "~8.2.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.11",
    "@angular/compiler": "~8.2.11",
    "@angular/core": "~8.2.11",
    "@angular/forms": "~8.2.11",
    "@angular/http": "^7.2.15",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.11",
    "@angular/platform-browser-dynamic": "~8.2.11",
    "@angular/platform-server": "~8.2.0",
    "@angular/pwa": "^0.803.20",
    "@angular/router": "~8.2.11",
    "@angular/service-worker": "~8.2.11",
    "@angularclass/hmr": "^2.1.3",
    "@bk/console-home": "./tarballs/bk-console-home-1.0.8.tgz",
    "@bk/credit-console-conf": "./tarballs/bk-credit-console-conf-1.0.5.tgz",
    "@bk/credit-risk": "./tarballs/bk-credit-risk-1.0.7.tgz",
    "@bk/credit-risk-conf": "./tarballs/bk-credit-risk-conf-1.0.6.tgz",
    "@bk/doc-viewer": "./tarballs/bk-doc-viewer-1.0.9.tgz",
    "@bk/fancy": "./tarballs/bk-fancy-1.0.18.tgz",
    "@bk/nested-list": "./tarballs/bk-nested-list-0.2.5.tgz",
    "@bk/qr-code": "./tarballs/bk-qr-code-0.1.6.tgz",
    "@wfh/assets-processer": "./tarballs/dr-core-assets-processer-1.1.24.tgz",
    "@wfh/express-app": "./tarballs/wfh-express-app-1.1.1.tgz",
    "@wfh/http-request-proxy": "./tarballs/dr-http-request-proxy-5.1.3.tgz",
    "@wfh/http-server": "./tarballs/dr-http-server-1.1.0.tgz",
    "@growth/pm2": "./tarballs/pm2-2.10.6-debug.1.tgz",
    "@nguniversal/express-engine": "8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^8.1.0",
    "@types/express": "^4.17.0",
    "@types/request-promise-native": "^1.0.15",
    "@wfh/plink": "./tarballs/@wfh/plink-0.8.16.tgz",
    "hammerjs": "2.0.8",
    "request-promise-native": "^1.0.5",
    "rxjs": "~6.4.0",
    "serve-static-zip": "./tarballs/serve-static-zip-1.0.3.tgz",
    "sleep-promise": "^8.0.0",
    "tslib": "^1.10.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "vconsole": "^3.3.4",
    "webpack-bundle-analyzer": "~3.6.0",
    "@bk/js-sdk-ocr": "./tarballs/bk-js-sdk-ocr-1.5.4.tgz",
    "core-js": "~3.1.4",
    "@wfh/ng-app-builder": "./tarballs/dr-core-ng-app-builder-2.1.18.tgz",
    "@bk/cra-scripts": "./tarballs/bk-cra-scripts-1.0.1.tgz",
    "@wfh/webpack2-builder": "./tarballs/dr-core-webpack2-builder-1.1.6.tgz",
    "@types/drcp-types": "./tarballs/types-drcp-types-1.0.0.tgz",
    "@angular-devkit/build-angular": "~0.803.12",
    "@angular/cli": "~8.3.12",
    "@angular/compiler-cli": "~8.2.11",
    "@angular/language-service": "~8.2.11",
    "@types/node": "^10.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "tslint": "~5.11.0",
    "ts-node": "~8.3.0"
  }
}
