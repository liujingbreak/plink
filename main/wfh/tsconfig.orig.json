{
	"extends": "./tsconfig-base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "package-settings": ["package-settings"],
      "*": [
        "../node_modules/@types/*",
        "../node_modules/*"
      ]
    },
    "typeRoots": [
      "../node_modules/@types/*",
      "types"
    ]
  },
  "include": [
    "ts/**/*.ts",
    "dist/**/*.d.ts",
    "../redux-toolkit-observable/**/*.ts",
    "../thread-promise-pool/ts/**/*.ts"
  ]
}
