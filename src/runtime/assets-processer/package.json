{
  "name": "@wfh/assets-processer",
  "version": "1.2.0-0",
  "description": "Work with Plink, copy assets to destDir folder only when devMode is false",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dr": {
    "type": [
      "builder",
      "server"
    ],
    "builderPriority": 8000,
    "serverPriority": 1000,
    "category": [
      "internal",
      "nodejs",
      "tool"
    ],
    "cli": "dist/cli.js#default",
    "setting": {
      "value": "isom/assets-processer-setting.js#defaultSetting",
      "type": "isom/assets-processer-setting#AssetsProcesserSetting"
    }
  },
  "--dep-for-charset-transform": {
    "@types/iconv": "~2.1.16",
    "iconv": "~2.3.5"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "rxjs": "~6.4.0",
    "glob": "^7.1.2",
    "request": "^2.88.2",
    "serve-favicon": "^2.4.4",
    "yazl": "2.5.1",
    "source-map-support": "~0.5.16",
    "lodash": "^4.17.21",
    "gulp-unzip": "1.0.0",
    "adm-zip": "^0.4.14",
    "pify": "~4.0.1",
    "http-proxy-middleware": "~1.0.6",
    "nodemailer": "^6.3.0",
    "serve-index": "^1.9.1",
    "yauzl": "~2.10.0",
    "ms": "~2.1.3",
    "fs-extra": "^5.0.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/lodash": "^4.14.92",
    "@types/yazl": "2.4.2",
    "@types/request": "~2.48.3",
    "@types/glob": "^7.1.1",
    "@types/nodemailer": "^6.2.1",
    "@types/serve-index": "^1.7.30",
    "@types/yauzl": "~2.9.1",
    "@types/fs-extra": "5.0.4",
    "@types/jasmine": "^3.9.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "MIT"
}
