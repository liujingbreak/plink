{
  "name": "@wfh/ng-app-builder",
  "version": "2.1.43-8",
  "description": "Angular 8 build based on skeleton generated by @angular/cli",
  "browser": "browser.ts",
  "main": "dist/server.js",
  "builders": "ts/build-angular/builders.json",
  "schematics": "./ts/ng/schematics/collection.json",
  "dr": {
    "typeRoot": "types",
    "category": [
      "tool",
      "internal",
      "nodejs"
    ],
    "angularCompiler": true,
    "type": [
      "builder",
      "server"
    ],
    "builderPriority": 9999,
    "ngTsHandler": "dist/server#tsHandler",
    "config": {
      "server": {
        "outputPath": "dist/static",
        "tsconfigInclude": [],
        "useThread": false,
        "ng8Compliant": true
      }
    },
    "setting": {
      "value": "isom/ng-app-builder-setting.js#defaultSetting",
      "type": "isom/ng-app-builder-setting#NgAppBuilderSetting"
    }
  },
  "peerDependencies": {
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "@angular/platform-server": "~8.2.14",
    "@types/karma": "^3.0.3",
    "@angularclass/hmr": "^2.1.3"
  },
  "dependencies": {
    "@types/express": "~4.17.6",
    "@types/glob": "^7.1.1",
    "semver": "^7.3.2",
    "@types/webpack": "~4.41.13",
    "cssnano": "^3.10.0",
    "webpack-hot-middleware": "2.22.1",
    "@types/webpack-dev-server": "^3.11.2",
    "fs-extra": "^5.0.0",
    "gzip-size": "^4.1.0",
    "pretty-bytes": "^5.0.0",
    "reflect-metadata": "^0.1.12",
    "domino": "2.0.3",
    "resolve-url-loader": "^2.3.0",
    "url-loader": "^2.1.0",
    "html-loader": "^0.5.5",
    "raw-loader": "3.1.0",
    "yaml-loader": "^0.5.0",
    "json-loader": "^0.5.7",
    "lru-cache": "^5.1.1",
    "rxjs": "~6.4.0"
  },
  "devDependencies": {
    "@types/semver": "^7.3.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "LJ",
  "license": "MIT"
}
