{
  "name": "plink-mono",
  "description": "This is a monorepo package, not for publishing",
  "version": "1.0.0",
  "private": true,
  "packages": [
    "src",
    "main/packages",
    "doc-app",
    "server-tool-space/conf",
    "conf"
  ],
  "plink": {
    "noModuleSymlink": ["main/packages"]
  },
  "scripts": {
    "lint-wfh": "eslint -c main/.eslintrc.js --ext .ts main/wfh/ts",
    "lint-redux": "eslint -c main/.eslintrc.js --ext .ts main/packages/redux-toolkit-observable",
    "lint-thread": "eslint -c main/.eslintrc.js --ext .ts main/packages/thread-promise-pool",
    "lint-main-file": "eslint -c main/.eslintrc.js",
    "lint-other": "eslint -c .eslintrc.js"
  },
  "devDependencies": {
    "eslint-config-react-app": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsdoc": "^35.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "babel-eslint": "^10.0.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.9.0",
    "typescript": "^4.1.2",
    "tslint": "^6.1.3"
  }
}
